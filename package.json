{
  "name": "waterborne-diseases",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-google-maps/api": "^2.20.8",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.0",
    "firebase": "^12.3.0",
    "framer-motion": "^11.18.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-leaflet": "^4.2.1",
    "react-loading-skeleton": "^3.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.3",
    "react-scripts": "5.0.1",
    "recharts": "^3.2.0",
    "swiper": "^11.2.10",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env CI=false react-scripts build",
    "build:ci": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "jsx-a11y/anchor-is-valid": [
        "warn",
        {
          "components": [
            "Link",
            "button"
          ],
          "specialLink": [
            "to",
            "onClick"
          ],
          "aspects": [
            "invalidHref",
            "preferButton"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.12"
  }
}
